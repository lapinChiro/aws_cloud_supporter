{
  "name": "aws-cloud-supporter",
  "version": "1.0.0",
  "description": "Generate CloudWatch metrics recommendations for CloudFormation templates",
  "main": "dist/cli/index.js",
  "bin": {
    "aws-cloud-supporter": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "p-limit": "^5.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "aws",
    "cloudformation",
    "cloudwatch",
    "metrics",
    "monitoring",
    "cli"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/aws-cloud-supporter.git"
  }
}
